{
  "name": "mechmania",
  "version": "2.6.3",
  "description": "CLI tools to do everything mechmania (test your bot, upload them to mm servers, queue games, etc.)",
  "main": "index.js",
  "repository": "https://github.com/pranaygp/mm-cli-tools",
  "author": "Pranay Prakash <pranay.gp@gmail.com>",
  "license": "MIT",
  "scripts": {
    "flow": "flow src/",
    "build": "flow-remove-types src/ --out-dir lib",
    "prepublish": "flow-remove-types src/ --out-dir lib"
  },
  "bin": {
    "mm": "lib/index.js"
  },
  "pkg": {
    "targets": [
      "node8-macos",
      "node8-linux",
      "node8-win"
    ],
    "scripts": [
      "lib/commands"
    ]
  },
  "dependencies": {
    "adm-zip": "^0.4.13",
    "app-root-path": "2.1.0",
    "chalk": "2.4.0",
    "execa": "2.0.4",
    "fetch": "1.1.0",
    "get-stream": "4.0.0",
    "inquirer": "6.2.0",
    "lodash": "4.17.13",
    "mkdirp": "0.5.1",
    "moment": "2.22.2",
    "node-fetch": "2.2.0",
    "ondeath": "^1.0.0",
    "opn": "6.0.0",
    "rimraf": "2.6.2",
    "tar": "4.4.6",
    "through2": "2.0.3",
    "update-check": "^1.5.2",
    "yargs": "12.0.2"
  },
  "devDependencies": {
    "eslint": "4.18.2",
    "flow-bin": "0.66.0",
    "flow-remove-types": "1.2.3",
    "pkg": "4.3.0"
  }
}
